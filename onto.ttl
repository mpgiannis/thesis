@prefix : <http://www.semanticweb.org/giannis/ontologies/2025/0/ProgramStudy#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sch: <http://www.schema.org/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/giannis/ontologies/2025/0/ProgramStudy#> .

<http://www.semanticweb.org/giannis/ontologies/2025/0/ProgramStudy> rdf:type owl:Ontology ;
                                                                     rdfs:comment "The model of the current program study." .

#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#qualifiedCardinality
owl:qualifiedCardinality rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.schema.org/coursePrerequisites
sch:coursePrerequisites rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf owl:topObjectProperty ;
                        rdf:type owl:TransitiveProperty ;
                        rdfs:domain sch:Course ;
                        rdfs:range sch:Course ;
                        rdfs:comment "Recommended prerequisites for this course." .


###  http://www.schema.org/hasCourse
sch:hasCourse rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              owl:inverseOf :belongsTo ;
              rdfs:domain sch:EducationalOccupationalProgram ;
              rdfs:range sch:Course ;
              rdfs:comment "The course that is offerd by this program study." .


###  http://www.schema.org/hasPart
sch:hasPart rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            owl:inverseOf sch:isPartOf ;
            rdfs:domain sch:Course ;
            rdfs:range sch:Course ;
            rdfs:comment "Specifing the laboratory part of the course." .


###  http://www.schema.org/isPartOf
sch:isPartOf rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:comment "Specifing in which course is part of." .


###  http://www.semanticweb.org/giannis/ontologies/2025/0/ProgramStudy#belongsTo
:belongsTo rdf:type owl:ObjectProperty ;
           rdfs:domain sch:Course ;
           rdfs:range sch:EducationalOccupationalProgram ;
           rdfs:comment "In which program study the course is offered." .


###  http://www.semanticweb.org/giannis/ontologies/2025/0/ProgramStudy#studentsCourse
:studentsCourse rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:domain :Student ;
                rdfs:range sch:Course ;
                rdfs:comment "Students course." .


#################################################################
#    Data properties
#################################################################

###  http://www.schema.org/about
sch:about rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:domain sch:Course ;
          rdfs:range xsd:string ;
          rdfs:comment "The scientific field from the course,possible values are \"Generic Knowledge and Skills (GKS), Programming and Algorithms (PA), Electronics (EL), Embedded Computation Systems (ECS), Communications and Networks (CN), Data Management - Artifial Inteligence (DMAI)\"." .


###  http://www.schema.org/additionalType
sch:additionalType rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf owl:topDataProperty ;
                   rdfs:domain sch:Course ;
                   rdfs:range xsd:string ;
                   rdfs:comment "The category of the course, possible values are \"Compulsory (CO), Compulsory Optional (CO-OP), Optional (OP)\"." .


###  http://www.schema.org/availableLanguage
sch:availableLanguage rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf owl:topDataProperty ;
                      rdfs:domain sch:Course ;
                      rdfs:range xsd:string ;
                      rdfs:comment "The available languages teaching and exam for the course." .


###  http://www.schema.org/courseCode
sch:courseCode rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain sch:Course ;
               rdfs:range xsd:string ;
               rdfs:comment "Code of the course." .


###  http://www.schema.org/courseMode
sch:courseMode rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain sch:Course ;
               rdfs:range xsd:string ;
               rdfs:comment "Specifing that the course is a laboratory." .


###  http://www.schema.org/educationalLevel
sch:educationalLevel rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf owl:topDataProperty ;
                     rdfs:domain [ rdf:type owl:Class ;
                                   owl:unionOf ( sch:Course
                                                 sch:EducationalOccupationalProgram
                                               )
                                 ] ;
                     rdfs:range xsd:string ;
                     rdfs:comment "Can refer to the semester that the course belongs or the EQF level from the program study." .


###  http://www.schema.org/hasCredential
sch:hasCredential rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf owl:topDataProperty ;
                  rdfs:domain sch:EducationalOccupationalProgram ;
                  rdfs:range xsd:string ;
                  rdfs:comment "The amount of ECTS student take by graduate." .


###  http://www.schema.org/learningResourceType
sch:learningResourceType rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf owl:topDataProperty ;
                         rdfs:domain sch:Course ;
                         rdfs:range xsd:string ;
                         rdfs:comment "The course type, possible values are \"General Background (GB), Special Background (SB), General Background - Skills Development (GB-SD), Scientific Area - Skills Development (SA-SD), Scientific Area (SA), Specialization - Skills Development (SP-SD), Specialization (SP), Skills Development (SD)\"." .


###  http://www.schema.org/name
sch:name rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( sch:Course
                                     sch:EducationalOccupationalProgram
                                   )
                     ] ;
         rdfs:range xsd:string ;
         rdfs:comment "Full name." .


###  http://www.schema.org/numberOfCredits
sch:numberOfCredits rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf owl:topDataProperty ;
                    rdfs:domain sch:Course ;
                    rdfs:range xsd:int ;
                    rdfs:comment "The amount of credits student can take from passing the course." .


###  http://www.schema.org/programPrerequisites
sch:programPrerequisites rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf owl:topDataProperty ;
                         rdfs:domain sch:EducationalOccupationalProgram ;
                         rdfs:range xsd:string ;
                         rdfs:comment "The prerequisites for somone in order to start this program." .


###  http://www.schema.org/repeatFrequency
sch:repeatFrequency rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf owl:topDataProperty ;
                    rdfs:domain sch:Course ;
                    rdfs:range xsd:string ;
                    rdfs:comment "The frequency of the course taught." .


###  http://www.schema.org/timeRequired
sch:timeRequired rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdfs:domain sch:Course ;
                 rdfs:range xsd:string ;
                 rdfs:comment "The hours needed every week for the course." .


###  http://www.schema.org/timeToComplete
sch:timeToComplete rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf owl:topDataProperty ;
                   rdfs:domain sch:EducationalOccupationalProgram ;
                   rdfs:range xsd:string ;
                   rdfs:comment "Time in years need to complete this program study." .


###  http://www.schema.org/url
sch:url rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf owl:topDataProperty ;
        rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( sch:Course
                                    sch:EducationalOccupationalProgram
                                  )
                    ] ;
        rdfs:range xsd:anyURI ;
        rdfs:comment "Source url for more info." .


###  http://www.semanticweb.org/giannis/ontologies/2025/0/ProgramStudy#inGroup
:inGroup rdf:type owl:DatatypeProperty ;
         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( sch:Course
                                     :Student
                                   )
                     ] ;
         rdfs:range xsd:string ;
         rdfs:comment "Specifing the group." .


#################################################################
#    Classes
#################################################################

###  http://www.schema.org/Course
sch:Course rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty sch:courseCode ;
                             owl:someValuesFrom xsd:string
                           ] .


###  http://www.schema.org/EducationalOccupationalProgram
sch:EducationalOccupationalProgram rdf:type owl:Class .


###  http://www.semanticweb.org/giannis/ontologies/2025/0/ProgramStudy#CoCourse
:CoCourse rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty sch:additionalType ;
                                owl:hasValue "Compulsory (CO)"
                              ] ;
          rdfs:subClassOf sch:Course .


###  http://www.semanticweb.org/giannis/ontologies/2025/0/ProgramStudy#ElesCourse
:ElesCourse rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Class ;
                                                         owl:complementOf :Student
                                                       ]
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :inGroup ;
                                                         owl:hasValue "ELES"
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
            rdfs:subClassOf sch:Course .


###  http://www.semanticweb.org/giannis/ontologies/2025/0/ProgramStudy#OptionalCourse
:OptionalCourse rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty sch:additionalType ;
                                      owl:hasValue "Optional (OP)"
                                    ] ;
                rdfs:subClassOf sch:Course .


###  http://www.semanticweb.org/giannis/ontologies/2025/0/ProgramStudy#PdaiCourse
:PdaiCourse rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Class ;
                                                         owl:complementOf :Student
                                                       ]
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :inGroup ;
                                                         owl:hasValue "PDAI"
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
            rdfs:subClassOf sch:Course .


###  http://www.semanticweb.org/giannis/ontologies/2025/0/ProgramStudy#Student
:Student rdf:type owl:Class .


